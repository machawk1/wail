{
  "name": "wail-electron",
  "version": "0.1.0",
  "main": "electron-main.js",
  "description": "Web Archiving Integration Layer (WAIL) is a graphical user interface (GUI) atop multiple web archiving tools intended to be used as an easy way for anyone to preserve and replay web page",
  "scripts": {
    "nightmare": "cross-env babel-node tools/testNightmare",
    "start-electron-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register electron-main-dev.js",
    "dev": "cross-env webpack-dev-server --port 9000 --content-base src --inline --hot",
    "start-dev": "cross-env concurrently --kill-others \"npm run start-electron-dev\" \"npm run dev\"",
    "copy-resources": "cross-env babel-node tools/copy",
    "test-xml": "cross-env babel-node tools/test-xml",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "standard": "standard --verbose | snazzy",
    "format-standard": "standard-format -w",
    "get-ojdk": "cross-env babel-node tools/downloadOpenJDK",
    "get-ojdk-all": "cross-env babel-node tools/downloadOpenJDK --all"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "webpack.*",
      "node_modules/*",
      "tools/*",
      "release/*",
      "*.cxml",
      "*.xml",
      "package.js",
      "dist/*",
      "README.md",
      "zips/*",
      "support",
      "config",
      "bundledApps"
    ],
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react",
      "node6"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "bundle-loader": "^0.5.4",
    "bytewise": "^1.1.0",
    "checksum": "^0.1.1",
    "cheerio": "^0.20.0",
    "cls-bluebird": "^1.1.3",
    "codemirror": "^5.16.0",
    "continuation-local-storage": "^3.1.7",
    "css-loader": "^0.23.1",
    "del": "2.2.1",
    "electron-debug": "^1.0.1",
    "electron-process": "^0.2.0",
    "electron-react-devtools": "^0.3.1",
    "electron-settings": "^1.0.4",
    "eventemitter3": "^1.2.0",
    "extract-zip": "^1.5.0",
    "flux": "2.1.1",
    "fs-extra": "0.30.0",
    "google-auth-library": "^0.9.8",
    "googleapis": "^9.0.0",
    "graceful-fs": "^4.1.4",
    "history": "^3.0.0",
    "insafe": "^0.3.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.13.1",
    "material-design-icons-iconfont": "^2.0.5",
    "material-ui": "^0.15.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "mz": "^2.4.0",
    "named-regexp": "^0.1.1",
    "ncp": "^2.0.0",
    "nightmare": "^2.5.2",
    "node-schedule": "^1.1.1",
    "normalize.css": "^4.1.1",
    "pkginfo": "^0.4.0",
    "postcss": "^5.0.21",
    "react": "15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-cellblock": "^2.0.0",
    "react-dom": "15.1.0",
    "react-router": "^2.5.1",
    "react-tap-event-plugin": "^1.0.0",
    "request": "^2.72.0",
    "request-debug": "^0.2.0",
    "request-promise": "^3.0.0",
    "roboto-fontface": "^0.5.0",
    "shelljs": "^0.7.0",
    "sliced": "^1.0.1",
    "sort-stream2": "^1.0.0",
    "source-map-support": "^0.4.0",
    "split2": "^2.1.0",
    "string": "^3.3.1",
    "through2": "^2.0.1",
    "unzip": "^0.1.11",
    "valid-url": "^1.0.9",
    "validator": "^5.4.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "6.9.2",
    "babelify": "^7.3.0",
    "classnames": "^2.2.5",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.8",
    "debug": "^2.2.0",
    "devtron": "^1.2.1",
    "electron-compile": "3.2.6",
    "electron-compilers": "3.1.2",
    "electron-devtools-installer": "^1.1.4",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.2.5",
    "electron-react-devtools": "0.3.1",
    "electron-rebuild": "^1.1.5",
    "electron-reload": "^1.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-jsx": "^1.2.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "highlight": "^0.2.4",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "raw-loader": "^0.5.1",
    "run-sequence": "^1.2.1",
    "sass-loader": "^3.2.1",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "standard-format": "^2.2.2",
    "standard-react": "^2.1.0",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-middleware": "^1.5.1",
    "webpack-typescript": "^0.5.6"
  },
  "devEngines": {
    "node": "5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
